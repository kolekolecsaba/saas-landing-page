{
    "version": 2,
    "public": true,
    "cleanUrls": true,
    "trailingSlash": false,
    "github": {
      "silent": true
    },
    "builds": [
      { "src": "*", "use": "@vercel/static" }
    ],
    "routes": [
      { "handle": "filesystem" },
      { "src": "/imgs/(.*)", "dest": "/imgs/$1" },
      { "src": "/js/(.*)", "dest": "/js/$1" },
      { "src": "/(.*)", "dest": "/$1" }
    ],
    "buildCommand": "chmod +x vercel.build.sh && ./vercel.build.sh"
  }
